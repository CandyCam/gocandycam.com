Options +FollowSymlinks
RewriteEngine On

RewriteRule ^ dist/%{REQUEST_URI}

RewriteRule ^(.*)\.html$ $1

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*)$ $1.html

RewriteCond %{HTTPS} !=on
RewriteCond %{HTTP_HOST} ^www\.(.+)$ [NC]
RewriteRule ^ http://%1%{REQUEST_URI} [R=301]